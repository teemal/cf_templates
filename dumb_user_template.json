{
  "Resources": {
    "User": {
      "Type": "AWS::IAM::User",
      "Properties": {
        "UserName": "dumb-boi",
        "LoginProfile": {
          "Password": "butt1234",
          "PasswordResetRequired": false
        },
        "Path": "/"
      }
    },
    "UserCredentials": {
      "Type": "AWS::IAM::AccessKey",
      "Properties": {
        "UserName": {
          "Ref": "User"
        }
      }
    },
    "DumbPolicy": {
      "Type": "AWS::IAM::ManagedPolicy",
      "Properties": {
        "Description": "user policy for almost no credentials",
        "ManagedPolicyName": "aws-iam-dumb-policy",
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "CloudDevIAMDumb",
              "Effect": "Allow",
              "Action": [
                "iam:ListUsers"
              ],
              "Resource": "*"
            }
          ]
        }
      }
    },
    "DumbRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "AWS": [
                  "arn:aws:iam::921245065062:user/dumb-boi"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        },
        "ManagedPolicyArns": [
          {
            "Ref": "DumbPolicy"
          }
        ],
        "Description": "Dumb role",
        "RoleName": "dumb"
      }
    },
    "DumbGroup": {
      "Type": "AWS::IAM::Group",
      "Properties": {
        "GroupName": "Dumb-group",
        "ManagedPolicyArns": [
          "arn:aws:iam::aws:policy/AWSCloudFormationReadOnlyAccess"
        ]
      }
    }
  }
}